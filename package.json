  },
  "devDependencies": {
    "@types/marked": "^4.0.3",
    "@types/node": "^18.0.5",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.19.0",
    "eslint-config-intolerable-style-guide": "^4.0.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
